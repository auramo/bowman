    "uuid": "^3.2.1",
    "webpack-node-externals": "^1.6.0"
  },
  "browserslist": [
    "last 2 versions"
  ],
  "babel": {
    "presets": [
      "env",
      "react"
    ],
    "plugins": [
      "babel-plugin-transform-object-rest-spread"
    ]
  }
}
